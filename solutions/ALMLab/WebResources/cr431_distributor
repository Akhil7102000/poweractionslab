function Zonalheadapproval(executionContext){
    var formcontext = executionContext.getFormContext();
    debugger;
    var Status_zonalhead=formcontext.getAttribute("cr431_approvalstatuszonalhead").getValue();

    if(Status_zonalhead == 254460004)
     {
        formcontext.getAttribute("cr431_approvalstatuszonalhead_").setValue(true);        
     }
    }
       
    function statehead(executionContext){
        var formcontext = executionContext.getFormContext();
        debugger;
        var statusstatehead=formcontext.getAttribute("cr431_approvalstatusstatehead").getValue();
    
        if(statusstatehead == 254460004)
         {
            formcontext.getAttribute("cr431_approval_statusstatehead").setValue(true);        
         }
        }



        function statusaccount(executionContext){
            var formcontext = executionContext.getFormContext();
            debugger;
            var approvalstatusaccount=formcontext.getAttribute("cr431_approvalstatusaccount").getValue();
        
            if(approvalstatusaccount == 254460004)
             {
                formcontext.getAttribute("cr431_approval_statusaccount").setValue(true);        
             }



    
            }


            
        function Kycverification(executionContext){
            var formcontext = executionContext.getFormContext();
            debugger;
            var approvalstatusmain=formcontext.getAttribute("cr431_statusmain").getValue();
        
            if(approvalstatusmain == 254460002)
             {
                formcontext.getAttribute("cr431_kycverification").setValue(true);        
             }
             else
             if(approvalstatusmain == 254460003)
             {
                formcontext.getAttribute("cr431_financialverification").setValue(true);        
             }



    
            }


            
            function financialverif(executionContext){
               var formcontext = executionContext.getFormContext();
               debugger;
               var financialverif_status=formcontext.getAttribute("cr431_financialverification").getValue();
               var approvalstatusmain11=formcontext.getAttribute("cr431_statusmain").getValue();

               if(approvalstatusmain11==254460003 && financialverif_status == true)
                {
                   formcontext.getAttribute("cr431_Approval").setValue(254460000);        
                }
               
   
   
   
       
               }











